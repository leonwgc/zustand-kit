{
  "name": "zustand-x",
  "version": "0.1.0",
  "description": "A lightweight and flexible state management library for React built on top of Zustand.",
  "keywords": [
    "zustand",
    "react",
    "state-management",
    "hooks"
  ],
  "license": "MIT",
  "repository": "https://github.com/leonwgc/zustand-x",
  "bugs": {
    "url": "https://github.com/leonwgc/zustand-x/issues"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "tsup && tsc",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "publish--patch": "./publish.sh patch",
    "publish--minor": "./publish.sh minor",
    "publish--major": "./publish.sh major"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "antd": "^6.1.4",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "packrs": "^1.0.1",
    "prettier": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "yargs": "^15.3.1",
    "zustand": "^5.0.9"
  },
  "files": [
    "dist",
    "types"
  ]
}
